{
  "android": {
    "root": "",
    "moduleName": "android",
    "dependencies": [
      "jp.co.cyberagent.android.gpuimage:gpuimage-library:1.3.0"
    ]
  },
  "ios": {
    "react-native@<0.61.0": {
      "copy": [{
        "source": "ios/**",
        "dest": "{{{projectName}}}/Libraries/LLImageView"
      }],
      "pbxproj": {
        "addProject": [{
          "path": "LLImageView/LLImageView.xcodeproj",
          "group": "Libraries",
          "staticLibs": [{
            "name": "libLLImageView.a",
            "target": "LLImageView"
          }]
        }],
        "addHeaderSearchPath": [
          "\"$(SRCROOT)/{{{projectName}}}/Libraries/LLImageView/**\""
        ]
      }
    }
  }
}
