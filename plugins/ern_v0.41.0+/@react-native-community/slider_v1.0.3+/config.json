{
  "android": {
    "root": "",
    "moduleName": "android"
  },
  "ios": {
    "react-native@<0.61.0": {
      "copy": [
        {
          "source": "ios/**",
          "dest": "{{{projectName}}}/Libraries/RNCSlider"
        }
      ],
      "pbxproj": {
        "addProject": [
          {
            "path": "RNCSlider/RNCSlider.xcodeproj",
            "group": "Libraries",
            "staticLibs": [
              {
                "name": "libRNCSlider.a",
                "target": "RNCSlider"
              }
            ]
          }
        ],
        "addHeaderSearchPath": [
          "\"$(SRCROOT)/{{{projectName}}}/Libraries/RNCSlider/**\""
        ]
      }
    }
  }
}
