{
  "android": {
    "root": "",
    "moduleName": "android"
  },
  "ios": {
    "react-native@<0.61.0": {
      "copy": [
        {
          "source": "ios/**",
          "dest": "{{{projectName}}}/Libraries/RNCNetInfo"
        }
      ],
      "pbxproj": {
        "addProject": [
          {
            "path": "RNCNetInfo/RNCNetInfo.xcodeproj",
            "group": "Libraries",
            "staticLibs": [
              {
                "name": "libRNCNetInfo.a",
                "target": "RNCNetInfo"
              }
            ]
          }
        ],
        "addHeaderSearchPath": [
          "\"$(SRCROOT)/{{{projectName}}}/Libraries/RNCNetInfo/**\""
        ]
      }
    }
  }
}
