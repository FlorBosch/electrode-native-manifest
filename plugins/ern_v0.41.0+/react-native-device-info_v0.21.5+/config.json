{
  "android": {
    "root": "",
    "moduleName": "android",
    "dependencies": [
      "com.google.android.gms:play-services-gcm:16.0.0"
    ]
  },
  "ios": {
    "react-native@<0.61.0": {
      "copy": [{
        "source": "ios/*",
        "dest": "{{{projectName}}}/Libraries/RNDeviceInfo"
      }],
      "pbxproj": {
        "addProject": [{
          "path": "RNDeviceInfo/RNDeviceInfo.xcodeproj",
          "group": "Libraries",
          "staticLibs": [{
            "name": "libRNDeviceInfo.a",
            "target": "RNDeviceInfo"
          }]
        }],
        "addHeaderSearchPath": [
          "\"$(SRCROOT)/{{{projectName}}}/Libraries/RNDeviceInfo/**\""
        ]
      }
    }
  }
}
