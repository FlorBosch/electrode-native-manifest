{
  "android": {
    "root": "",
    "moduleName": "android"
  },
  "ios": {
    "react-native@<0.61.0": {
      "copy": [{
        "source": "ios/**",
        "dest": "{{{projectName}}}/Libraries/LLUploader"
      }],
      "pbxproj": {
        "addProject": [{
          "path": "LLUploader/LLUploader.xcodeproj",
          "group": "Libraries",
          "staticLibs": [{
            "name": "libLLUploader.a",
            "target": "LLUploader"
          }]
        }],
        "addHeaderSearchPath": [
          "\"$(SRCROOT)/{{{projectName}}}/Libraries/LLUploader/**\""
        ]
      }
    }
  }
}
