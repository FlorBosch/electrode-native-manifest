{
  "android": {
    "root": "",
    "moduleName": "android"
  },
  "ios": {
    "react-native@<0.61.0": {
      "copy": [
        { "source": "ios/*", "dest": "{{{projectName}}}/Libraries/RNSVG" }
      ],
      "pbxproj": {
        "addProject": [
          { "path": "RNSVG/RNSVG.xcodeproj", "group": "Libraries", "staticLibs": [ { "name": "libRNSVG.a", "target": "RNSVG" } ] }
        ],
        "addHeaderSearchPath": [
          "\"$(SRCROOT)/{{{projectName}}}/Libraries/RNSVG/**\""
        ]
      }
    }
  }
}
