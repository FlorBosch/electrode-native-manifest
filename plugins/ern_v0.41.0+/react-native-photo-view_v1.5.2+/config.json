{
  "android": {
    "root": "",
    "moduleName": "android",
    "dependencies": [
      "com.facebook.fresco:fresco:0.11.0",
      "me.relex:photodraweeview:1.0.0"
    ]
  },
  "ios": {
    "react-native@<0.61.0": {
      "copy": [{
        "source": "ios/**",
        "dest": "{{{projectName}}}/Libraries/RNPhotoView"
      }],
      "pbxproj": {
        "addProject": [{
          "path": "RNPhotoView/RNPhotoView.xcodeproj",
          "group": "Libraries",
          "staticLibs": [{
            "name": "libRNPhotoView.a",
            "target": "RNPhotoView"
          }]
        }],
        "addHeaderSearchPath": [
          "\"$(SRCROOT)/{{{projectName}}}/Libraries/RNPhotoView/**\""
        ]
      }
    },
    "react-native@>=0.61.0": {
      "podspec": "react-native-photo-view.podspec"
    }
  }
}
